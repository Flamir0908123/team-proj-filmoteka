{{#each this}}
<div class="movies-card">
    <img src="https://image.tmdb.org/t/p/w500{{poster_path}}" alt="{{title}}" width="320" />

    <div class="description">
        <p class="description-item">    
            {{title}}
        </p>
        <p class="description-item">  
            {{genre_ids}}
        </p>
        <p class="description-item">   
            {{release_date}}
        </p>
    </div>
</div>
{{/each}}